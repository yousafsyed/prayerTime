{
    "name": "Islamic Prayer Times",
    "manifest_version": 2,
    "version": "1.0.0",
    "description": "Get Accurate Prayer times",
    "browser_action": {
        "name": "Islamic Prayer Times",
	    "default_popup": "demo.html",
	    "default_icon": "icon.png"
    },

    "background": {
        "scripts": ["prayerTime.js", "background.js"]
    },
    "permissions": ["notifications", "background","storage","https://maxcdn.bootstrapcdn.com/","geolocation"]
}